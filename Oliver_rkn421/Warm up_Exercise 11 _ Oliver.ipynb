    "from sympy.abc import x\n",
    "\n",
    "def tnFactory(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    if n==1:\n",
    "        return x\n",
    "    else:\n",
    "        return 2*x*tnFactory(n-1)-tnFactory(n-2)\n",
    "    \n",
    "print(tnFactory(0))\n",
    "print(tnFactory(1))\n",
    "print(tnFactory(2))"
   ]
